{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/react-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "inertiajs/inertia-laravel": "^2.0.6",
        "laravel/framework": "^12.28.1",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.12",
        "spatie/laravel-data": "^4.17.1",
        "spatie/laravel-enum": "^3.2.0",
        "spatie/laravel-typescript-transformer": "^2.5.2",
        "tpetry/laravel-postgresql-enhanced": "^3.1",
        "tpetry/laravel-query-expressions": "^1.5",
        "league/flysystem-aws-s3-v3": "^3.29.0",
        "laravel/cashier": "^15.7.1",
        "filament/filament": "^4.0.10",
        "filament/spatie-laravel-settings-plugin": "^4.0.10",
        "aws/aws-sdk-php": "^3.356.16",
        "prism-php/prism": "^v0.78.0",
        "saloonphp/laravel-plugin": "^3.6",
        "saloonphp/saloon": "^3.14",
        "laravel/socialite": "^5.23",
        "laravel/reverb": "^1.5.1",
        "laravel/nightwatch": "^1.13.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.24.1",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.45",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": "^4.1.0",
        "pestphp/pest-plugin-browser": "^4.1.0",
        "pestphp/pest-plugin-laravel": "^v4.0.0",
        "pestphp/pest-plugin-type-coverage": "^4.0.2",
        "larastan/larastan": "^3.7.1",
        "laravel-lang/lang": "^15.24.5",
        "laravel-lang/starter-kits": "^1.3.10",
        "rector/rector": "^2.1.7",
        "barryvdh/laravel-debugbar": "^3.16.0",
        "driftingly/rector-laravel": "^2.0.7",
        "laravel/boost": "^1.1.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan lang:update",
            "@php artisan boost:install"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr --kill-others"
        ],
        "test:lint": [
            "pint --test",
            "npm run format:check",
            "@test:refactor"
        ],
        "refactor": "rector",
        "test:refactor": "rector --dry-run",
        "test:types": "phpstan analyse",
        "test:arch": "pest --filter=arch",
        "test:type-cov": "pest --type-coverage --min=100",
        "test:unit-cov": "php ./vendor/bin/pest --parallel --coverage",
        "test:unit": "php ./vendor/bin/pest --parallel",
        "lint": [
            "pint --parallel",
            "npm run format",
            "@refactor"
        ],
        "test": [
            "@test:lint",
            "@test:types",
            "@test:arch",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
